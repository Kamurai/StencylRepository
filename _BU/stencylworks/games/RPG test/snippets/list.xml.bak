ies" hidden="false" id="10" name="_WeaponProperties" order="15">
            <text order="0" value="A:MinDamage=1"/>
            <text order="1" value="A:MaxDamage=10"/>
            <text order="2" value="A:DamageType=Punch"/>
            <text order="3" value="A:WeaponType=Glove"/>
        </list>
